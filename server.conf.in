[global]
tree.py: cherrypy.Application(ogs.Root())
log.error_file: '@LOG_ERROR_FILE@'
log.access_file: '@LOG_ACCESS_FILE@'

[/]
tools.staticdir.root: ogs.current_dir
tools.staticdir.on = True
tools.staticdir.dir = "web"
tools.staticdir.index: "index.html"

[/data]
tools.staticdir.on = True
tools.staticdir.dir = "@DATA_ROOT@"

[/chat/ws]
tools.websocket.on: True
tools.websocket.handler_cls: websocket_chat.ChatWebSocketHandler

[/pi/ws]
tools.websocket.on: True
tools.websocket.handler_cls: websocket_pi.PiWebSocketHandler
